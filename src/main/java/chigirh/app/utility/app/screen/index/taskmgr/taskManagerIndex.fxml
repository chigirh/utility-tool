<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import chigirh.app.utility.javafx.component.UtlTextField?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chigirh.app.utility.app.screen.index.taskmgr.TaskManagerIndexPresenter">
   <children>
      <AnchorPane layoutX="-0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox layoutX="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <AnchorPane>
                     <children>
                        <ScrollPane fx:id="taskScroll" prefHeight="300.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="0.0">
                           <content>
                              <VBox fx:id="taskBox" prefWidth="600.0" />
                           </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
                  <AnchorPane>
                     <children>
                        <HBox prefHeight="25.0">
                           <children>
                              <UtlTextField fx:id="taskAddTf" prefHeight="23.0" prefWidth="490.0" />
                              <AnchorPane prefWidth="10.0" />
                              <Button fx:id="taskAddBt" mnemonicParsing="false" onAction="#onTaskAdd" prefHeight="30.0" prefWidth="100.0" text="新規追加" />
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children></AnchorPane>
